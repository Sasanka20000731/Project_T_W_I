    <script>

        document.addEventListener("DOMContentLoaded", function () {
            
         var a = '1';
         var b = '2';
         var m = '3';
         var t = '4';

            var uLevel = '@ViewBag.UserRole';

            if (uLevel == m)
            {
                 $('#AddUserID').hide();
                 $('#AddPrinterID').hide();
                 $('#AllocationID').hide();
                 $('#DeleteTicketID').hide();
                 $('#ReportID').hide();
                 $('#BranchID').hide();
                 $('#OpenCloseTicketID').hide();
             }

            if (uLevel == b)
            {
                $('#AddUserID').hide();
                $('#AddPrinterID').hide();
                $('#AllocationID').hide();
                $('#DeleteTicketID').hide();
                $('#ReportID').hide();
                $('#BranchID').hide();
                $('#UserManagementID').hide();
                $('#AddAllocation').hide();
                $('#AudiTrailID').hide();
                $('#OpenCloseTicketID').hide();
            }

            if (uLevel == t) {
                $('#AddTicketID').hide();
                $('#PrinterManagementID').hide();
                $('#AllocationID').hide();
                $('#DeleteTicketID').hide();
                $('#ReportID').hide();
                $('#BranchID').hide();
                $('#UserManagementID').hide();
                $('#AddAllocation').hide();
                $('#AudiTrailID').hide();
                $('#OpenCloseTicketID').hide();
            }
         });

    </script>


